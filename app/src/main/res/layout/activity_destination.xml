<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.google.android.material.appbar.MaterialToolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/default_color"
            tools:ignore="MissingConstraints">

            <TextView
                android:id="@+id/toolbar_title"
                style="@style/TextAppearance.AppCompat.Widget.ActionBar.Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="@string/destination"
                android:textColor="@android:color/white" />
        </com.google.android.material.appbar.MaterialToolbar>


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="336dp"
            android:layout_height="168dp"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/angkorwat"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="77dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="157dp"
            android:layout_height="157dp"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/koh_norea"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="285dp" />

        <View
            android:id="@+id/rectangle_1"
            android:layout_width="334dp"
            android:layout_height="168dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/gradient_carousel"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="77dp" />

        <TextView
            android:id="@+id/visit_angko"
            android:layout_width="129.78dp"
            android:layout_height="20dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:gravity="top"
            android:text="@string/visit_angko"
            android:textAppearance="@style/visit_angko"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="67dp"
            tools:layout_editor_absoluteY="118dp" />

        <View
            android:id="@+id/ellipse_211"
            android:layout_width="8dp"
            android:layout_height="8dp"
            android:alpha="0.15"
            android:background="#1BA169"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="181dp"
            tools:layout_editor_absoluteY="256dp" />

        <View
            android:id="@+id/ellipse_3"
            android:layout_width="8dp"
            android:layout_height="8dp"
            android:alpha="0.15"
            android:background="#1BA169"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="227dp"
            tools:layout_editor_absoluteY="256dp" />

        <View
            android:id="@+id/ellipse_"
            android:layout_width="8dp"
            android:layout_height="8dp"
            android:alpha="0.15"
            android:background="#1BA169"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="168dp"
            tools:layout_editor_absoluteY="256dp" />

        <View
            android:id="@+id/ellipse_213"
            android:layout_width="8dp"
            android:layout_height="8dp"
            android:background="#1BA169"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="196dp"
            tools:layout_editor_absoluteY="256dp" />

        <View
            android:id="@+id/ellipse_2"
            android:layout_width="8dp"
            android:layout_height="8dp"
            android:alpha="0.15"
            android:background="#1BA169"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="211dp"
            tools:layout_editor_absoluteY="256dp" />

        <View
            android:id="@+id/bg"
            android:layout_width="158dp"
            android:layout_height="158dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/bg_product"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="285dp" />

        <TextView
            android:id="@+id/koh_norea_i"
            android:layout_width="142dp"
            android:layout_height="20dp"
            android:layout_alignParentTop="true"
            android:gravity="top"
            android:text="@string/koh_norea_i"
            android:textAppearance="@style/koh_norea_i"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="26dp"
            tools:layout_editor_absoluteY="384dp" />

        <TextView
            android:id="@+id/km_away"
            android:layout_width="79.65dp"
            android:layout_height="15dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="top"
            android:text="@string/km_away"
            android:textAppearance="@style/km_away"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="45dp"
            tools:layout_editor_absoluteY="412dp" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="14dp"
            android:layout_height="16dp"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/ic_location"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="24dp"
            tools:layout_editor_absoluteY="412dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
